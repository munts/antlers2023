<flynt-component name="NavigationFooterColumns" class="componentSpacing" data-theme="dark">
  <div class="container" data-flow="layout">
  {% if preContentHtml %}
      <div data-size="medium" data-align="left">
        {{ preContentHtml|e('wp_kses_post') }}
      </div>
    {% endif %}
  </div>
  <div class="containerTwo" data-flow="layout">
    <div class="col-1">
      {# {{footerLogo|join(', ')}} #}
      {% set rootPath = fn('get_home_url') %}
      <img src="{{footerLogo|join(', ')}}" style="max-width:320px;" alt="Antlers footer logo">
    </div>
    <div class="col-2">
      Antlers at Vail<br>
      680 Lionshead Place<br>
      Vail, Colorado 81657<br>
      USA<br><br>
      Reservations: <a href="tel:1-888-268-5377">1-888-268-5377</a><br>
      Front Desk/Guest Services: <a href="tel:970-476-2471">970-476-2471</a><br>
      Email: <a href="mailto:info@antlersvail.com">info@antlersvail.com</a>
    </div>
    {% if menu %}
      <nav class="navigation right-menu" aria-label="{{ labels.ariaLabel }}">
        {% include 'Partials/_menu.twig' with { 'items': menu.get_items, 'level': '0', 'maxLevel': maxLevel } only %}
      </nav>
    {% endif %}
  </div>
</flynt-component>
