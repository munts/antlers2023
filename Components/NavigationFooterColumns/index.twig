<flynt-component name="NavigationFooterColumns" class="componentSpacing" data-theme="dark">
  <div class="container" data-flow="layout">
  {% if preContentHtml %}
      <div data-size="medium" data-align="left">
        {{ preContentHtml|e('wp_kses_post') }}
      </div>
    {% endif %}
  </div>
  <div class="containerTwo" data-flow="layout" style="display:flex;flex-direction:row;justify-content:space-around;">
    <div class="col-1">
      {# {{footerLogo|join(', ')}} #}
      {% set rootPath = fn('get_home_url') %}
      <img src="{{footerLogo|join(', ')}}" style="max-width:380px;">
    </div>
    <div class="col-2">680 Lionshead Place<br>
      Vail, Colorado 81657<br>
      USA<br><br>
      Reservations: 1-888268-5377<br>
      Ftont Desk/Guest Services: 970-476-2471<br>
      Email: info@antlersvail.com
    </div>
    {% if menu %}
      <nav class="navigation" aria-label="{{ labels.ariaLabel }}">
        {% include 'Partials/_menu.twig' with { 'items': menu.get_items, 'level': '0', 'maxLevel': maxLevel } only %}
      </nav>
    {% endif %}
  </div>
</flynt-component>
